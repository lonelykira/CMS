package com.shishuo.cms.entity;

import java.io.Serializable;

public class VideoFactory extends BaseEntity implements Serializable{
	private static final long serialVersionUID = 1L;
	private Long videoFactoryId;
	private String videoUrl; // 视频地址
	private String videoTitle; // 视频标题
	private String tag; // 标签
	private String frontCoverUrl; // 封面图片地址
	private String introduction; // 简介
	private String type; // 分类

	
	public Long getVideoFactoryId() {
		return videoFactoryId;
	}
	public void setVideoFactoryId(Long videoFactoryId) {
		this.videoFactoryId = videoFactoryId;
	}
	public String getVideoUrl() {
		return videoUrl;
	}
	public void setVideoUrl(String videoUrl) {
		this.videoUrl = videoUrl;
	}
	public String getVideoTitle() {
		return videoTitle;
	}
	public void setVideoTitle(String videoTitle) {
		this.videoTitle = videoTitle;
	}
	public String getTag() {
		return tag;
	}
	public void setTag(String tag) {
		this.tag = tag;
	}
	public String getFrontCoverUrl() {
		return frontCoverUrl;
	}
	public void setFrontCoverUrl(String frontCoverUrl) {
		this.frontCoverUrl = frontCoverUrl;
	}
	public String getIntroduction() {
		return introduction;
	}
	public void setIntroduction(String introduction) {
		this.introduction = introduction;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	
}
